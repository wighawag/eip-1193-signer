{
	"name": "eip-1193-signer",
	"version": "0.1.0",
	"description": "generic signer for eip-1193 providers",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	".": {
		"require": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.cjs"
		},
		"import": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.mjs"
		}
	},
	"devDependencies": {
		"eip-1193": "^0.6.1",
		"pkgroll": "^2.5.1",
		"rimraf": "^6.0.1",
		"typescript": "^5.7.2"
	},
	"dependencies": {
		"viem": "^2.21.51"
	},
	"scripts": {
		"build": "rimraf dist && pkgroll --sourcemap",
		"dev": "pkgroll --watch"
	}
}
